{
  "name": "nem-starter",
  "version": "0.0.1",
  "description": "Node Express Mongoose starter, to kick in production deployment project with ES6/ES7 features.",
  "main": "index.js",
  "scripts": {
    "build": "nps build",
    "start": "nps",
    "test": "nps test",
    "test:watch": "nps test.watch",
    "dev": "nps dev",
